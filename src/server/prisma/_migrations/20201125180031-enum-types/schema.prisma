generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  id                 Int                @id @default(autoincrement())
  name               String?
  email              String             @unique
  age                Int
  address            String?
  typeUserInCampaign TypeUserInCampaign @default(INSPETOR)
  typeUser           TypeUser           @default(USER)
}

enum TypeUserInCampaign {
  INSPETOR
  SUPERVISOR
  ADMIN
}

enum TypeUser {
  USER
  SUPERADMIN
}
